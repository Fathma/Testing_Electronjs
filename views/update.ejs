<%- include('partials/head') %>
<input type="hidden"  id="lan" name="lan" value="<%= la %>" >


<div class="well">
  <form id="update">
    <img  alt="" src="<%= data1.img %>" id='img'><br>
    <input type="hidden"  id="_id" name="_id" value="<%= data1.UserId %>" >
    <label for="" id="fn">First name</label>
    <input type="text" class="form-control" id="firstname" name="firstname" value="<%= data1.FirstName %>">
    <br>
    <label for="" id="ln">Last name</label>
    <input type="text" class="form-control" id="lastname" name="lastname" value="<%= data1.LastName %>">
    <br><br>
    <label for="" id="agee">Age</label>
    <input type="text" class="form-control" id="age" name="age" value="<%= data1.Age %>">
    <br><br>
    <input type="submit" value="Submit"> 
  </form>
  <button class="btn btn-warning" id="closeBtn">Pop Up</button>
</div>

<script src="../controllers/update.js"></script>

<script src="../controllers/header1.js"></script>

<script>if (typeof module === 'object') {window.module = module; module = undefined;}</script>
<script src="../static/scripts/webcam.min.js"></script>
